{
    "files.associations": {
        "*.ct_*": "json",
        "vs_*.sc": "glsl",
        "fs_*.sc": "glsl",
        "cs_*.sc": "glsl",
        "varying.def.sc": "glsl",
        "*.inc": "c",
        "*.h": "c",
        "*.cpp": "cpp",
        "limits": "cpp",
        "ratio": "cpp",
        "valarray": "cpp",
        "algorithm": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "initializer_list": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "span": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "ios": "cpp",
        "regex": "cpp"
    },
    "[json]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "todo-tree.filtering.excludeGlobs": [
        "zig",
        "zig-out",
        "zig-cache",
        "externals",
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "checkbox",
        },
        "NOTE": {
            "icon": "note",
        },
        "USEFUL": {
            "icon": "verified",
        },
        "COMMENT": {
            "icon": "comment",
        },
        "LEARN": {
            "icon": "bookmark",
        },
        "FIXME": {
            "icon": "tools",
        },
        "RECHECK": {
            "icon": "codescan",
        },
        "INCOMPLETE": {
            "icon": "alert",
        },
        "BUG": {
            "icon": "bug",
        },
        "SEE NOTES": {
            "icon": "note",
        },
        "POST": {
            "icon": "share",
        },
        "[ ]": {
            "icon": "check",
        },
        "[x]": {
            "icon": "check",
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "SEE NOTES",
        "HACK",
        "FIXME",
        "RECHECK",
        "INCOMPLETE",
        "TODO",
        "NOTE",
        "POST",
        "USEFUL",
        "LEARN",
        "COMMENT",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
    "lldb.displayFormat": "auto",
    "lldb.showDisassembly": "auto",
    "lldb.dereferencePointers": true,
    "lldb.consoleMode": "evaluate",
    "dotnet.defaultSolution": "disable",
}